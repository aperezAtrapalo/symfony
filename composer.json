{
    "name": "makhan/makhan",
    "type": "library",
    "description": "The Makhan PHP framework",
    "keywords": ["framework"],
    "homepage": "https://makhan.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Master Makhan",
            "email": "master@makhan.com"
        },
        {
            "name": "Makhan Community",
            "homepage": "https://makhan.com/contributors"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "doctrine/common": "~2.4",
        "twig/twig": "~1.23|~2.0",
        "psr/cache": "~1.0",
        "psr/log": "~1.0",
        "makhan/polyfill-intl-icu": "~1.0",
        "makhan/polyfill-mbstring": "~1.0",
        "makhan/polyfill-php56": "~1.0",
        "makhan/polyfill-php70": "~1.0",
        "makhan/polyfill-util": "~1.0"
    },
    "replace": {
        "makhan/asset": "self.version",
        "makhan/browser-kit": "self.version",
        "makhan/cache": "self.version",
        "makhan/class-loader": "self.version",
        "makhan/config": "self.version",
        "makhan/console": "self.version",
        "makhan/css-selector": "self.version",
        "makhan/dependency-injection": "self.version",
        "makhan/debug": "self.version",
        "makhan/debug-bundle": "self.version",
        "makhan/doctrine-bridge": "self.version",
        "makhan/dom-crawler": "self.version",
        "makhan/event-dispatcher": "self.version",
        "makhan/expression-language": "self.version",
        "makhan/filesystem": "self.version",
        "makhan/finder": "self.version",
        "makhan/form": "self.version",
        "makhan/framework-bundle": "self.version",
        "makhan/http-foundation": "self.version",
        "makhan/http-kernel": "self.version",
        "makhan/intl": "self.version",
        "makhan/ldap": "self.version",
        "makhan/monolog-bridge": "self.version",
        "makhan/options-resolver": "self.version",
        "makhan/process": "self.version",
        "makhan/property-access": "self.version",
        "makhan/property-info": "self.version",
        "makhan/proxy-manager-bridge": "self.version",
        "makhan/routing": "self.version",
        "makhan/security": "self.version",
        "makhan/security-core": "self.version",
        "makhan/security-csrf": "self.version",
        "makhan/security-guard": "self.version",
        "makhan/security-http": "self.version",
        "makhan/security-bundle": "self.version",
        "makhan/serializer": "self.version",
        "makhan/stopwatch": "self.version",
        "makhan/templating": "self.version",
        "makhan/translation": "self.version",
        "makhan/twig-bridge": "self.version",
        "makhan/twig-bundle": "self.version",
        "makhan/validator": "self.version",
        "makhan/var-dumper": "self.version",
        "makhan/web-profiler-bundle": "self.version",
        "makhan/yaml": "self.version"
    },
    "require-dev": {
        "cache/integration-tests": "dev-master",
        "doctrine/cache": "~1.6",
        "doctrine/data-fixtures": "1.0.*",
        "doctrine/dbal": "~2.4",
        "doctrine/orm": "~2.4,>=2.4.5",
        "doctrine/doctrine-bundle": "~1.4",
        "monolog/monolog": "~1.11",
        "ocramius/proxy-manager": "~0.4|~1.0|~2.0",
        "predis/predis": "~1.0",
        "egulias/email-validator": "~1.2",
        "makhan/polyfill-apcu": "~1.1",
        "makhan/security-acl": "~2.8|~3.0",
        "phpdocumentor/reflection-docblock": "^3.0"
    },
    "conflict": {
        "phpdocumentor/reflection-docblock": "<3.0"
    },
    "autoload": {
        "psr-4": {
            "Makhan\\Bridge\\Doctrine\\": "src/Makhan/Bridge/Doctrine/",
            "Makhan\\Bridge\\Monolog\\": "src/Makhan/Bridge/Monolog/",
            "Makhan\\Bridge\\ProxyManager\\": "src/Makhan/Bridge/ProxyManager/",
            "Makhan\\Bridge\\Swiftmailer\\": "src/Makhan/Bridge/Swiftmailer/",
            "Makhan\\Bridge\\Twig\\": "src/Makhan/Bridge/Twig/",
            "Makhan\\Bundle\\": "src/Makhan/Bundle/",
            "Makhan\\Component\\": "src/Makhan/Component/"
        },
        "classmap": [
            "src/Makhan/Component/Intl/Resources/stubs"
        ],
        "exclude-from-classmap": [
            "**/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Makhan\\Bridge\\PhpUnit\\": "src/Makhan/Bridge/PhpUnit/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "3.2-dev"
        }
    }
}
