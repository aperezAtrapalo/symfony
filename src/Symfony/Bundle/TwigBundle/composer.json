{
    "name": "makhan/twig-bundle",
    "type": "makhan-bundle",
    "description": "Makhan TwigBundle",
    "keywords": [],
    "homepage": "https://makhan.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@makhan.com"
        },
        {
            "name": "Makhan Community",
            "homepage": "https://makhan.com/contributors"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "makhan/asset": "~2.8|~3.0",
        "makhan/twig-bridge": "~2.8|~3.0",
        "makhan/http-foundation": "~2.8|~3.0",
        "makhan/http-kernel": "~2.8|~3.0"
    },
    "require-dev": {
        "makhan/stopwatch": "~2.8|~3.0",
        "makhan/dependency-injection": "~2.8|~3.0",
        "makhan/expression-language": "~2.8|~3.0",
        "makhan/config": "~2.8|~3.0",
        "makhan/finder": "~2.8|~3.0",
        "makhan/routing": "~2.8|~3.0",
        "makhan/templating": "~2.8|~3.0",
        "makhan/yaml": "~2.8|~3.0",
        "makhan/framework-bundle": "~2.8|~3.0"
    },
    "autoload": {
        "psr-4": { "Makhan\\Bundle\\TwigBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "3.2-dev"
        }
    }
}
