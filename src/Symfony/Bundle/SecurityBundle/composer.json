{
    "name": "makhan/security-bundle",
    "type": "makhan-bundle",
    "description": "Makhan SecurityBundle",
    "keywords": [],
    "homepage": "https://makhan.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@makhan.com"
        },
        {
            "name": "Makhan Community",
            "homepage": "https://makhan.com/contributors"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "makhan/security": "~3.1",
        "makhan/http-kernel": "~2.8|~3.0",
        "makhan/polyfill-php70": "~1.0"
    },
    "require-dev": {
        "makhan/browser-kit": "~2.8|~3.0",
        "makhan/console": "~2.8|~3.0",
        "makhan/css-selector": "~2.8|~3.0",
        "makhan/dom-crawler": "~2.8|~3.0",
        "makhan/form": "~2.8|~3.0",
        "makhan/framework-bundle": "~2.8|~3.0",
        "makhan/http-foundation": "~2.8|~3.0",
        "makhan/security-acl": "~2.8|~3.0",
        "makhan/twig-bundle": "~2.8|~3.0",
        "makhan/twig-bridge": "~2.8|~3.0",
        "makhan/process": "~2.8|~3.0",
        "makhan/validator": "~2.8|~3.0",
        "makhan/yaml": "~2.8|~3.0",
        "makhan/expression-language": "~2.8|~3.0",
        "doctrine/doctrine-bundle": "~1.4",
        "twig/twig": "~1.23|~2.0"
    },
    "suggest": {
        "makhan/security-acl": "For using the ACL functionality of this bundle"
    },
    "autoload": {
        "psr-4": { "Makhan\\Bundle\\SecurityBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "3.2-dev"
        }
    }
}
