{
    "name": "makhan/monolog-bridge",
    "type": "makhan-bridge",
    "description": "Makhan Monolog Bridge",
    "keywords": [],
    "homepage": "https://makhan.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@makhan.com"
        },
        {
            "name": "Makhan Community",
            "homepage": "https://makhan.com/contributors"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "monolog/monolog": "~1.11"
    },
    "require-dev": {
        "makhan/http-kernel": "~2.8|~3.0",
        "makhan/console": "~2.8|~3.0",
        "makhan/event-dispatcher": "~2.8|~3.0"
    },
    "suggest": {
        "makhan/http-kernel": "For using the debugging handlers together with the response life cycle of the HTTP kernel.",
        "makhan/console": "For the possibility to show log messages in console commands depending on verbosity settings. You need version ~2.3 of the console for it.",
        "makhan/event-dispatcher": "Needed when using log messages in console commands."
     },
    "autoload": {
        "psr-4": { "Makhan\\Bridge\\Monolog\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "3.2-dev"
        }
    }
}
