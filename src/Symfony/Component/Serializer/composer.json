{
    "name": "makhan/serializer",
    "type": "library",
    "description": "Makhan Serializer Component",
    "keywords": [],
    "homepage": "https://makhan.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@makhan.com"
        },
        {
            "name": "Makhan Community",
            "homepage": "https://makhan.com/contributors"
        }
    ],
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "makhan/yaml": "~2.8|~3.0",
        "makhan/config": "~2.8|~3.0",
        "makhan/property-access": "~2.8|~3.0",
        "makhan/http-foundation": "~2.8|~3.0",
        "makhan/cache": "~3.1",
        "makhan/property-info": "~2.8|~3.0",
        "doctrine/annotations": "~1.0",
        "doctrine/cache": "~1.0"
    },
    "conflict": {
        "makhan/property-access": ">=3.0,<3.0.4|>=2.8,<2.8.4"
    },
    "suggest": {
        "psr/cache-implementation": "For using the metadata cache.",
        "makhan/property-info": "To deserialize relations.",
        "makhan/yaml": "For using the default YAML mapping loader.",
        "makhan/config": "For using the XML mapping loader.",
        "makhan/property-access": "For using the ObjectNormalizer.",
        "makhan/http-foundation": "To use the DataUriNormalizer.",
        "doctrine/annotations": "For using the annotation mapping. You will also need doctrine/cache.",
        "doctrine/cache": "For using the default cached annotation reader and metadata cache."
    },
    "autoload": {
        "psr-4": { "Makhan\\Component\\Serializer\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "3.2-dev"
        }
    }
}
