{
    "name": "makhan/translation",
    "type": "library",
    "description": "Makhan Translation Component",
    "keywords": [],
    "homepage": "https://makhan.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@makhan.com"
        },
        {
            "name": "Makhan Community",
            "homepage": "https://makhan.com/contributors"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "makhan/polyfill-mbstring": "~1.0"
    },
    "require-dev": {
        "makhan/config": "~2.8|~3.0",
        "makhan/intl": "~2.8|~3.0",
        "makhan/yaml": "~2.8|~3.0",
        "psr/log": "~1.0"
    },
    "conflict": {
        "makhan/config": "<2.8"
    },
    "suggest": {
        "makhan/config": "",
        "makhan/yaml": "",
        "psr/log": "To use logging capability in translator"
    },
    "autoload": {
        "psr-4": { "Makhan\\Component\\Translation\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "3.2-dev"
        }
    }
}
