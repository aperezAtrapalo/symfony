{
    "name": "makhan/security-http",
    "type": "library",
    "description": "Makhan Security Component - HTTP Integration",
    "keywords": [],
    "homepage": "https://makhan.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@makhan.com"
        },
        {
            "name": "Makhan Community",
            "homepage": "https://makhan.com/contributors"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "makhan/security-core": "~2.8|~3.0",
        "makhan/event-dispatcher": "~2.8|~3.0",
        "makhan/http-foundation": "~2.8|~3.0",
        "makhan/http-kernel": "~2.8|~3.0",
        "makhan/polyfill-php56": "~1.0",
        "makhan/polyfill-php70": "~1.0",
        "makhan/property-access": "~2.8|~3.0"
    },
    "require-dev": {
        "makhan/routing": "~2.8|~3.0",
        "makhan/security-csrf": "~2.8|~3.0",
        "psr/log": "~1.0"
    },
    "suggest": {
        "makhan/security-csrf": "For using tokens to protect authentication/logout attempts",
        "makhan/routing": "For using the HttpUtils class to create sub-requests, redirect the user, and match URLs"
    },
    "autoload": {
        "psr-4": { "Makhan\\Component\\Security\\Http\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "3.2-dev"
        }
    }
}
