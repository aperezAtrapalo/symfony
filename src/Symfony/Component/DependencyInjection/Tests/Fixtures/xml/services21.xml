<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://makhan.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://makhan.com/schema/dic/services http://makhan.com/schema/dic/services/services-1.0.xsd">
  <services>
    <service id="foo" class="Foo">
      <factory method="createFoo">
        <service class="FooFactory">
          <factory method="createFooFactory">
            <service class="Foobar"/>
          </factory>
        </service>
      </factory>
      <configurator method="configureFoo">
        <service class="Bar">
          <configurator method="configureBar">
            <service class="Baz"/>
          </configurator>
        </service>
      </configurator>
    </service>
  </services>
</container>
