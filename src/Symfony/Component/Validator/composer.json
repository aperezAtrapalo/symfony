{
    "name": "makhan/validator",
    "type": "library",
    "description": "Makhan Validator Component",
    "keywords": [],
    "homepage": "https://makhan.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@makhan.com"
        },
        {
            "name": "Makhan Community",
            "homepage": "https://makhan.com/contributors"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "makhan/polyfill-mbstring": "~1.0",
        "makhan/translation": "~2.8|~3.0"
    },
    "require-dev": {
        "makhan/http-foundation": "~2.8|~3.0",
        "makhan/intl": "~2.8|~3.0",
        "makhan/yaml": "~2.8|~3.0",
        "makhan/config": "~2.8|~3.0",
        "makhan/expression-language": "~2.8|~3.0",
        "makhan/cache": "~3.1",
        "doctrine/annotations": "~1.0",
        "doctrine/cache": "~1.0",
        "egulias/email-validator": "~1.2,>=1.2.1"
    },
    "suggest": {
        "psr/cache-implementation": "For using the metadata cache.",
        "doctrine/annotations": "For using the annotation mapping. You will also need doctrine/cache.",
        "doctrine/cache": "For using the default cached annotation reader and metadata cache.",
        "makhan/http-foundation": "",
        "makhan/intl": "",
        "makhan/yaml": "",
        "makhan/config": "",
        "egulias/email-validator": "Strict (RFC compliant) email validation",
        "makhan/property-access": "For using the Expression validator",
        "makhan/expression-language": "For using the Expression validator"
    },
    "autoload": {
        "psr-4": { "Makhan\\Component\\Validator\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "3.2-dev"
        }
    }
}
