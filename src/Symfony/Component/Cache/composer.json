{
    "name": "makhan/cache",
    "type": "library",
    "description": "Makhan implementation of PSR-6",
    "keywords": ["caching", "psr6"],
    "homepage": "https://makhan.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Grekas",
            "email": "p@tchwork.com"
        },
        {
            "name": "Makhan Community",
            "homepage": "https://makhan.com/contributors"
        }
    ],
    "provide": {
        "psr/cache-implementation": "1.0"
    },
    "require": {
        "php": ">=5.5.9",
        "psr/cache": "~1.0",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "cache/integration-tests": "dev-master",
        "doctrine/cache": "~1.6",
        "predis/predis": "~1.0"
    },
    "suggest": {
        "makhan/polyfill-apcu": "For using ApcuAdapter on HHVM"
    },
    "autoload": {
        "psr-4": { "Makhan\\Component\\Cache\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "3.2-dev"
        }
    }
}
